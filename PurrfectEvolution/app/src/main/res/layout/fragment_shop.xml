<?xml version="1.0" encoding="utf-8"?>
<!--layout editori ein toimi jos käytössä on jokin muu kuin relativelayout -->
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="purrfect.evolution.ShopFragment"
    android:visibility="visible"
    app:columnCount="10"
    app:rowCount="30"
    android:background="@drawable/kissatausta1">


    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        app:srcCompat="@drawable/kauppa"
        android:scaleType="fitXY"/>

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="240dp"
        android:layout_marginLeft="120dp"
        android:layout_marginBottom="50dp"
        android:layout_marginRight="50dp"
        app:srcCompat="@drawable/catler" />
</RelativeLayout>



